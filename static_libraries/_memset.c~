#include "holberton.h"
/**
 * _memset - fills the first n bytes of the memory area pointed to by s,
 * with the constant byte8
 * @s: first pointer
 * @b: string to fill
 * @n: number of positions in 2D array
 *
 * Return: a pointer to memory area
 */
char *_memset(char *s, char b, unsigned int n)
{
	unsigned int j;

	for (j = 0; j < n; j++)
	{
		s[j] = b;
	}

	return (s);
}
