/**
 *new_dog - creates a new dog
 *@name: name of dog
 *@age: age of dog
 *@owner: owner of dog
 *Return: Null if function fails
 */

#include "dog.h"
#include <stdio.h>
#include <stddef.h>

dog_t *new_dog(char *name, float age, char *owner)
{
	if (new_dog != NULL)
	{
		my_dog.name = name;
		my_dog.age = age;
		my_dog.owner = owner;
	}
	else
		return (NULL);
}
